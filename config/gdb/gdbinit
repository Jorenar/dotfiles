set print object on
set print pretty on
set print vtbl

set disassembly-flavor intel

set tui border-kind acs
set tui border-mode normal
set tui active-border-mode bold

set pagination off
tui new-layout src+asm {-horizontal src 2 asm 1} 2 status 0 cmd 1
tui focus cmd
tui layout src+asm
tui disable
set pagination on

set history save on
set history size unlimited
set history remove-duplicates unlimited
